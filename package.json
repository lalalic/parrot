{
  "name": "parrot",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start1": "EXPO_DEVTOOLS_LISTEN_ADDRESS=172.20.10.4 REACT_NATIVE_PACKAGER_HOSTNAME=172.20.10.4 expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "yarn node --inspect node_modules/jest/bin/jest.js",
    "postinstall": "patch-package"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/src/__tests__/jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "<rootDir>/src/__tests__/jest-setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|react-router-native|redux-persist-expo-filesystem|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@react-native-community/slider": "^4.3.3",
    "@react-native-picker/picker": "2.4.2",
    "@react-native-voice/voice": "https://github.com/lalalic/speech-to-text#v3.2.40",
    "@reduxjs/toolkit": "^1.8.6",
    "@viro-community/react-viro": "^2.23.0",
    "cheerio": "^1.0.0-rc.12",
    "expo": "~46.0.16",
    "expo-av": "^12.0.4",
    "expo-build-properties": "~0.3.0",
    "expo-calendar": "~10.3.0",
    "expo-camera": "~12.3.0",
    "expo-dev-client": "^1.3.1",
    "expo-document-picker": "~10.3.0",
    "expo-file-system": "^14.1.0",
    "expo-image-manipulator": "~10.4.0",
    "expo-image-picker": "~13.3.1",
    "expo-print": "^11.3.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "fast-xml-parser": "^4.0.11",
    "patch-package": "^6.5.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-armotion": "../react-native-armotion",
    "react-native-calendars": "^1.1291.0",
    "react-native-fs": "^2.20.0",
    "react-native-tts": "^4.1.0",
    "react-native-video": "^5.2.1",
    "react-native-web": "~0.18.7",
    "react-native-webview": "11.23.0",
    "react-redux": "^8.0.4",
    "react-router-native": "^6.4.2",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-filesystem": "^2.0.0",
    "word-diff": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "jest": "^26.6.3",
    "jest-expo": "^46.0.1",
    "react-test-renderer": "^18.2.0"
  },
  "private": true,
  "main": "./index.js"
}
